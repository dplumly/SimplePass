{
    "manifest_version": 3,
    "name": "SimplePass",
    "description": "Make passwords simple",
    "version": "1.0",
    "icons": {
      "48": "SimplePass.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["fonts/*.ttf", "fonts/*.woff2", "fonts/*.woff"],
        "extension_ids": ["*"],
        "matches": ["*://*/*"],
        "resources": ["js/lottie.min.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ]
  }




