{
  "manifest_version": 3,
  "name": "SimplePass",
  "description": "No more confusing lookalike characters",
  "version": "1.0",
  "icons": {
    "48": "SimplePass.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/*.ttf",
        "fonts/*.woff2",
        "fonts/*.woff",
        "js/lottie.min.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}
